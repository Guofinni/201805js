//15.
        var a = 9;
        function ss() {
            a = 0;
            return function (b) {
                return b + (a++)
            }
        }
        var f = ss();
        var m = f(5);
        console.log(m);
        var n = ss()(5);
        console.log(n);
        var x = f(5);
        console.log(x);
